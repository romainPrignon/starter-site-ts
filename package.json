{
  "name": "starter-site-ts",
  "version": "0.0.0",
  "description": "React SPA starting point",
  "private": true,
  "engines": {
    "node": "^10",
    "npm": "^6.3"
  },
  "scripts": {
    "dev": "BROWSER=none react-scripts start",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage || true",
    "build": "react-scripts build",
    "build:dev": "tsc --noEmit --pretty",
    "start": "node ./server/index.js",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run build:dev && npm run test:coverage"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^6.3.1",
    "express": "^4.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "typescript": "^3.3.3"
  }
}
