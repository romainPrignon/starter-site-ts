{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "plugins": [
    "import",
    "react"
  ],
  "parserOptions": {
    "ecmaVersion":2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "jest": true,
    "es6": true,
    "browser": true,
    "serviceworker": true
  },
  "rules": {
    "no-console": ["off"],
    "react/prop-types": ["off"]
  }
}
